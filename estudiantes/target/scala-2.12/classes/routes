# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /listado                                    controllers.HomeController.indexList()
GET     /crear                                      controllers.HomeController.indexCrear()
POST    /crear                                      controllers.HomeController.crear()
POST	/search               						controllers.HomeController.search()
GET     /buscar/:identificacion                     controllers.HomeController.buscar(identificacion : Long)
GET     /eliminar/:identificacion                   controllers.HomeController.eliminar(identificacion : Long)


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
